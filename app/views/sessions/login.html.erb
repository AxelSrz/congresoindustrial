<div class="content">
	<div class="container">
		<div class="main">
			<h1>Inicia sesión como Administrador</h1>
			<hr>
			<%= form_tag(:action => 'login_attempt') do %>
			<p>Email:</br> <%= text_field_tag(:email) %></p>
			<p>Password:</br> <%= password_field_tag :login_password %></p>
			<%= submit_tag "Log In", class: "btn"  %>
			<% end %>
		</div>
		<div class="aside">
			<h3>Tu cuenta</h3>
			<p>No has iniciado sesión</p>
		</div>
	</div>
</div>
